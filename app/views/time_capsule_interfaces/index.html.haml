#time_capsule_interfaces
  - TimeCapsuleInterface::INTERFACES.each do |interface|
    .graph_container
      %h2= interface
      .time_capsule_interface_graph{:data => {:interface => interface}}
      .time_capsule_interface_archive_graph
  
= javascript_tag do
  == var time_capsule_interfaces_data = #{TimeCapsuleInterface.to_json_flot(@data).html_safe}
  == var time_capsule_interfaces_archive_data = #{TimeCapsuleInterface::Archive.to_json_flot(@archive_data).html_safe}